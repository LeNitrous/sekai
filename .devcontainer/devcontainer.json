{
  "name": "Sekai",
  "context": "..",
  "dockerFile": "../.codespaces.dockerfile",
  "privileged": true,
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "editorconfig.editorconfig"
      ]
    }
  },
  "mounts": [
    "source=${localEnv:XAUTHORITY},target=/home/vscode/.Xauthority,type=bind,consistency=cached"
  ],
  "remoteEnv": {
    "DISPLAY": "${localEnv:DISPLAY}"
  },
  "runArgs": [
    "--net=host"
  ]
}