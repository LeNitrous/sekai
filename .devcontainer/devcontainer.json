{
  "name": "Sekai",
  "context": "..",
  "dockerFile": "../.codespaces.dockerfile",
  "privileged": true,
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "editorconfig.editorconfig"
      ]
    }
  },
  "mounts": [
    {
      "source": "/tmp/.X11-unix",
      "target": "/tmp/.X11-unix",
      "type": "bind"
    }
  ],
  "remoteEnv": {
    "DISPLAY": "${localEnv:DISPLAY}"
  },
  "runArgs": [
    "--net=host"
  ]
}